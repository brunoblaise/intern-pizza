{
	"name": "pizza",
	"version": "1.0.0",
	"description": "",
	"main": "server.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon server.js",
		"migrate": "npx sequelize-cli db:migrate",
		"migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
		"seed": "npx sequelize-cli db:seed:all",
		"seed:reset": "npx sequelize-cli db:seed:undo:all && npm run seed",
		"reset": "npm run migrate:reset && npm run seed:reset",
		"test": "cross-env NODE_ENV=test mocha --testTimeout=100000 ",
		"pretest": "cross-env NODE_ENV=test npm run reset",
		"coverage": "npm run pretest && jest --coverage && cat ./coverage/lcov.info | coveralls"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.1.0",
		"celebrate": "^15.0.1",
		"config": "^3.3.9",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"google": "^2.1.0",
		"googleapis": "^123.0.0",
		"jsonwebtoken": "^9.0.1",
		"morgan": "^1.10.0",
		"nodemailer": "^6.9.4",
		"pg": "^8.11.1",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.32.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.0"
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		],
		"exclude": [
			"test/**"
		]
	},
	"devDependencies": {
		"chai": "^4.3.7",
		"chai-http": "^4.4.0",
		"coveralls": "^3.1.1",
		"istanbul": "^0.4.5",
		"jest": "^29.6.2",
		"mocha": "^10.2.0",
		"mocha-lcov-reporter": "^1.3.0",
		"nyc": "^15.1.0",
		"sequelize-cli": "^6.6.1",
		"supertest": "^6.3.3"
	}
}
